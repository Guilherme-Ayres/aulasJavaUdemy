package Entities;

public class ImportedProduct extends Product {

	private Integer customFee;

	public ImportedProduct(String name, Double price, Integer customFee) {
		super(name, price);
		this.customFee = customFee;
	}

	public Integer getCustomFee() {
		return customFee;
	}

	public void setCustomFee(Integer customFee) {
		this.customFee = customFee;
	}

	public Double totalPrice() {
		return getPrice() * customFee;
	}

	@Override
	public String priceTag() {
		return getName() + " $ " + totalPrice() + "Custom fee: " + customFee + ")";

	}

}
